QWidget {
}
QToolBar{
spacing : 6px;
}
QToolBar#hbar QPushButton{
spacing : 2px;
border : 2px groove lightgray;
min-height : 35px;
min-width  : 35px;
margin : 3px;
border-radius : 17px;
}
QToolBar#hbar QPushButton::hover{
border: 1px inset gray;

}
QPushButton[outilsBar="true"]{
border : 2px groove lightgray;
min-height : 50px;
min-width  : 50px;
padding: 2px;
border-radius : 25px;
}
QPushButton[outilsBar="true"]::hover{
border: 1px inset gray;
}
QPushButton[outilsBar="false"]{
border : 2px groove lightgray;
min-height : 50px;
min-width  : 50px;
margin: 0 20px 0 20px;
border-radius : 25px;
}
QPushButton[outilsBar="false"]::hover{
border: 1px inset gray;
}
QPushButton#nvAction{
image : url(:/ressources/outilsBar/nvAction.png);
}
QPushButton#ouvrirAction{

image : url(:/ressources/outilsBar/ouvrirAction.png);
}
QPushButton#svgdAction{

image : url(:/ressources/outilsBar/svgdAction.png);
}
QPushButton#exportAction{
image : url(:/ressources/outilsBar/exportAction.png);
}
QPushButton#exportAction::menu-indicator {
    subcontrol-position: center bottom;
    subcontrol-origin: padding;

    width:10px;
  }

QPushButton#simDP{

image : url(:/ressources/outilsBar/simD.png);
}

QPushButton#simDP::checked{

image : url(:/ressources/outilsBar/simP.png);
}
